<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns:p="http://www.springframework.org/schema/p"
xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<!-- because <context:annotation-config> activates the annotations only for the beans already registered in the application context. -->
<context:annotation-config/>

<bean name="address" class="com.springcore.Autowire.Annotation.Address">
<property name="villName" value="Gadai Saray"/>
<property name="city" value="Hajipur"/>
<property name="state" value="Bihar"/>
<property name="country" value="India"/>
</bean>
<bean name="employee" class="com.springcore.Autowire.Annotation.Employee" />

</beans>
 